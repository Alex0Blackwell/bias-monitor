{ "manifest_version": 2, 
    "name": "Bias Monitor", 
    "version": "0.1.0", 
    "permissions": ["<all_urls>"], 
    "browser_action": 
        { "default_popup": "index.html" },
    "content_scripts" : [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["page_text.js"]
        }
    ]
}